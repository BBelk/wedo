<%= simple_form_for(@list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.hidden_field :date_completed %>
    <%= f.input :due_date %>
  </div>

<div class="item-form" >
	<%= render 'items/form' %>
</div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
